{
  "name": "bootstrap-sandbox",
  "version": "1.0.0",
  "scripts": {
    "start": "rm -rf dist && rm -rf .parcel-cache && parcel serve src/index.html --public-url / --dist-dir dist",
    "build": "rm -rf dist && rm -rf .parcel-cache && parcel build src/index.html --public-url / --dist-dir dist",
    "open:chrome": "npx opn-cli http://localhost:1234 --browser=chrome",
    "serve": "npm-run-all --parallel start open:chrome"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@parcel/transformer-sass": "^2.13.3",
    "npm-run-all": "^4.1.5",
    "opn-cli": "^5.0.0",
    "parcel": "^2.13.3"
  }
}
